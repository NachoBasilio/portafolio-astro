---
import Proyecto from "./Proyecto";
const Proyectos = [{
    titulo: "Rubrica Pokemon",
    descripcion: "Guarda tus equipos de Pokémon de cada juego y generación en esta página. ¡Entra ahora!",
    imagen: "https://upload.wikimedia.org/wikipedia/commons/thumb/5/53/Pok%C3%A9_Ball_icon.svg/2052px-Pok%C3%A9_Ball_icon.svg.png",
    link: "https://ignadev.store/"
}]
---



<div class="container m-auto p-4 md:w-1/2 lg:w-1/4">
    <h3 class="text-4xl font-bold pl-4">Mis proyectos:</h3>
    {
        Proyectos.map((proyecto) => {
            return <Proyecto titulo={proyecto.titulo} descripcion={proyecto.descripcion} imagen={proyecto.imagen} link={proyecto.link} />
        })
    }
</div>